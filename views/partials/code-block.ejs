<div class="display">
    <% if (user) { %>

    <tr>
        <% snippet.forEach(function(t) { %>
        <form action="/snippets/<%= t.id %>?_method=DELETE" class="delete-form"
            method="POST">
            <div class="box">
                <h1 class="title is-4">

                    <button href="/snippets/<%= t.id %>"
                        class="delete"></button>
        </form>


        <td> Syntax: <%= t.language %><br></td>
        <td> Syntax: <%= t %><br></td>
        <h1 class="title is-4">
            <td> Name: <%= t.name %></td>
        </h1>
        <div class="code-block">
            <pre><code data-lang="<%= t.language %>"><%= t.snippet %></code></pre>

    </tr>

</div>
</div>
<% }) %>
<% } else { %>
<h1>Login to see Snippets</h1>
<% } %>
</div>
</div>